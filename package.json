{
  "name": "@mangoart/gatsby-plugin-purechat",
  "description": "Adds the PureChat Chat widget to a Gatsby page",
  "version": "1.0.2",
  "author": "Markus Oehler <m.oehler@digital-city.solutions>",
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/MangoArt/gatsby-plugin-purechat/issues"
  },
  "homepage": "https://github.com/MangoArt/gatsby-plugin-purechat",
  "repository": "github:MangoArt/gatsby-plugin-purechat.git",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "release": "release-it"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "purechat"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "babel-preset-gatsby-package": "0.2.2",
    "cross-env": "5.2.0",
    "husky": "3.0.3",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "release-it": "12.3.5"
  }
}
